//DBプロバイダー
datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

//
generator client {
  provider = "prisma-client-js"
  output   = "../generated/prisma"
}

model Setting {
  id               Int    @id @default(autoincrement())
  seikyuuGaku      Int
  company          String
  hinmoku          String
  tax              Int
  shiharaibi       String
  jogai            String
  createdAT DateTime @default(now())
}