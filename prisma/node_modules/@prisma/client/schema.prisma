//DBプロバイダー
datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

//
generator client {
  provider = "prisma-client-js"
  //  output   = "../generated/prisma"
  output   = "node_modules/@prisma/client"
}

model Setting {
  id        Int      @id @default(autoincrement())
  item      String   @unique
  keywords  String
  createdAT DateTime @default(now())
}
